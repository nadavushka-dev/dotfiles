# BEGIN_KITTY_THEME
# Tokyo Night
include current-theme.conf
# END_KITTY_THEME
#
# BEGIN_KITTY_FONTS
font_family      family="SpaceMono Nerd Font Mono"
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size 18.0
# END_KITTY_FONTS

# BEGIN_KEY_MAPS
map cmd+l toggle_layout tall
# END_KEY_MAPS

#BEGIN_MODES
fullscreen yes
